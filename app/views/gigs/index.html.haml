- content_for :additional_head_info do
  = auto_discovery_link_tag

%h1 Gigs

= link_to 'Add a gig!', new_gig_path

%table
  %tr
    %th Title
    %th Organization
    %th Location
    %th Post date
    %th Expire date

  - @gigs.each do |gig|
    %tr
      %td= link_to gig.title, gig
      %td= link_to_org(gig)
      %td= gig.location
      %td= gig.post_at
      %td= gig.expire_at

%br
